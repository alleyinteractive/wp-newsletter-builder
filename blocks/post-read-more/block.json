{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wp-newsletter-builder/post-read-more",
	"version": "0.1.0",
	"title": "Newsletter Post Read More",
	"category": "design",
	"icon": "admin-post",
	"description": "Displays a post read more button",
	"textdomain": "wp-newsletter-builder",
	"editorScript": "file:index.ts",
	"style": [
		"file:style-index.css"
	],
	"supports": {
		"inserter": false
	},
	"render": "file:render.php",
	"attributes": {
		"readMoreText": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": ["postId"]
}